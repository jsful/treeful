{
  "name": "webpack-boilerplate",
  "version": "2.1.0",
  "description": "A boilerplate that utilizes Webpack with React",
  "repository": "git@github.com:justinjung90/webpack-boilerplate.git",
  "author": "Justin Jung",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "build": "npm run lint && npm run clean && npm run build:webpack",
    "lint": "eslint src",
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js"
  },
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^5.1.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "file-loader": "^0.8.5",
    "history": "^3.0.0",
    "html-webpack-plugin": "^2.19.0",
    "node-sass": "^3.4.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.0.0-rc5",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  }
}
