    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^5.1.1",
    "expect": "^1.20.1",
    "express": "^4.13.3",
    "mocha": "^2.5.3",
    "node-sass": "^3.7.0",
    "openurl": "^1.1.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "rimraf": "^2.5.2",
